    }
   },
   "outputs": [],
   "source": [
    "# Create the list of structure fields--> three parameters ---> Name, DataType, Nulables\n",
    "data_schema = [ StructField('age',IntegerType(),True),\n",
    "                  StructField('name',StringType(),True)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
